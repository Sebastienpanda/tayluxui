{
	"name": "taylux",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"format": "prettier --write \"**/*.{ts,tsx,vue,md,json,css,scss}\"",
		"format:check": "prettier --check \"**/*.{ts,tsx,vue,md,json,css,scss}\"",
		"typecheck": "turbo run typecheck",
		"clean": "turbo clean && rm -rf node_modules **/node_modules",
		"reinstall": "pnpm clean && pnpm install"
	},
	"devDependencies": {
		"@types/node": "^22.9.3",
		"@typescript-eslint/eslint-plugin": "^8.15.0",
		"@typescript-eslint/parser": "^8.15.0",
		"@vitest/coverage-v8": "^2.1.5",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-vue": "^9.31.0",
		"prettier": "^3.3.3",
		"sass": "^1.81.0",
		"tsup": "^8.3.5",
		"turbo": "^2.3.1",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vite-plugin-dts": "^4.3.0",
		"vitest": "^2.1.5",
		"vue-eslint-parser": "^9.4.3"
	},
	"packageManager": "pnpm@9.14.2"
}
